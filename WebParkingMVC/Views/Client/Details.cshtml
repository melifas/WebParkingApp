@model LibraryWebParking.Model.Clients

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Profile</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("Dashboard_1", "Dashboards")">Home</a>
            </li>
            <li>
                <a>Extra Pages</a>
            </li>
            <li class="active">
                <strong>Profile</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<div class="wrapper wrapper-content">
    <div class="row animated fadeInRight">
        <div class="col-md-4">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Πληροφορίες Προφιλ</h5>
                </div>
                <div>
                    <div class="ibox-content no-padding border-left-right">
                        <img alt="image" class="img-responsive" src="@Url.Content(Model.ImagePath)">
                    </div>
                    <div class="ibox-content profile-content">
                        <h4>
                            <strong>
                                <i class="fa fa-male"></i>  @Model.FirstName  @Model.LatName
                            </strong>
                        </h4>
                        <h5>
                            <i class="fa fa-mobile-phone"></i>  @Html.DisplayFor(model => model.Phone)

                        </h5>
                        <h5>
                            <i class="fa fa-mail-forward"></i>  @Html.DisplayFor(model => model.Email)
                        </h5>
                        <div class="row m-t-lg">
                            <div class="col-md-4">
                                <span class="bar">5,3,9,6,5,9,7,3,5,2</span>
                                <h5><strong>169</strong> Posts</h5>
                            </div>
                            <div class="col-md-4">
                                <span class="line">5,3,9,6,5,9,7,3,5,2</span>
                                <h5><strong>28</strong> Following</h5>
                            </div>
                            <div class="col-md-4">
                                <span class="bar">5,3,2,-1,-3,-2,2,3,5,2</span>
                                <h5><strong>240</strong> Followers</h5>
                            </div>
                        </div>
                        <div class="user-button">
                            <div class="row">
                                <div class="col-md-12">
                                    <button type="button" class="btn btn-primary btn-sm btn-block" ><i class="fa fa-anchor"></i> <a style="color:white" href="@Url.Action("searchBooking", "SearchBooikings")">Πίσω στην Αναζήτηση</a></button>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



@Html.ActionLink("Αναζητηση", "searchBooking", "SearchBooikings")

@Html.ActionLink("Αναζητηση", "searchBooking", "SearchBooikings", new { }, new { @class = "text-primary" })

@Url.Action("Dashboard_1", "Dashboards")

<a href="@Url.Action("searchBooking", "SearchBooikings")">Home</a>



@section Scripts {
    @Scripts.Render("~/plugins/peity")

    <script type="text/javascript">
        $(document).ready(function () {

            $("span.pie").peity("pie", {
                fill: ['#1ab394', '#d7d7d7', '#ffffff']
            })

            $(".line").peity("line", {
                fill: '#1ab394',
                stroke: '#169c81',
            })

            $(".bar").peity("bar", {
                fill: ["#1ab394", "#d7d7d7"]
            })


        });
    </script>
}



